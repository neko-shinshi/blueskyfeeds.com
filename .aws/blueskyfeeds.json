{
  "family": "blueskyfeeds-com",
  "networkMode": "awsvpc",
  "executionRoleArn": "ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "blueskyfeeds-com",
      "image": "113989972184.dkr.ecr.us-east-1.amazonaws.com/blueskyfeeds-com:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/blueskyfeeds-com",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "/ecs/blueskyfeeds-com"
        }
      },
      "portMappings": [
        {
          "containerPort": 9123,
          "hostPort": 9123,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}